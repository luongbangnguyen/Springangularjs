<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="20150321_user" author="bangnl">
        <createTable tableName="user">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="email" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="fistName" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
             <column name="lastName" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
             <column name="password" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
             <column name="department" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
             <column name="dateOfBirth" type="datetime">
                <constraints nullable="true"/>
            </column>
            <column name="gender" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
            <column name="mobile" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
                <column name="landLine" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
                <column name="image" type="varchar(50)">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>